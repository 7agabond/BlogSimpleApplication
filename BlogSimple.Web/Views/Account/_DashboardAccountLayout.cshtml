@using Microsoft.AspNetCore.Identity;
@inject UserManager<User> UserManager

<!DOCTYPE html>
<html lang="en">

<partial name="_DashboardHead" />

<body class="sb-nav-fixed">
    <!-- Responsive navbar-->
    @await Component.InvokeAsync("AdminTopNav")

    <div id="layoutSidenav">
        <!-- Profile Side Nav-->
        <div id="layoutSidenav_nav">
            @await Component.InvokeAsync("ProfileSideNav");
        </div>
        <div id="layoutSidenav_content">
            <main>
                @RenderBody()
            </main>
        </div>
    </div>

    <div class="loader"></div>
    <!-- Footer-->
    @await Component.InvokeAsync("Footer")


    <partial name="_DashboardScripts" />
    <script src="~/js/admin-side-navbar.js" asp-append-version="true"></script>
    <script src="~/js/loader.js" asp-append-version="true"></script>
</body>
</html>
